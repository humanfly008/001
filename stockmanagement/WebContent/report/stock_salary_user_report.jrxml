	<property name="ireport.y" value="25"/>
	<parameter name="sql" class="java.lang.String"/>
	<parameter name="month" class="java.lang.String"/>
	<parameter name="year" class="java.lang.String"/>
	<parameter name="month_th" class="java.lang.String"/>
	<parameter name="year_th" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 	USER_ID,
	FIRST_NAME,
	LAST_NAME,
	ID_CARD,
	POSITION,
	PAY_TYPE,
	SALARY,

	SOCIAL_INSURANCE,
	TAX,
	DAILY_DAY,
	DAILY,
	FARE,
	DILIGENCE,
	BONUS,

	OT_PER_HOUR,
	OT_DATE,
	OT_HOUR,
	OT_RATE,
	OT_SUMMARY,
	(CASE
		WHEN PAY_TYPE = 'MONTH' THEN SALARY+FARE+DILIGENCE+BONUS+OT_SUMMARY
		ELSE DAILY+FARE+DILIGENCE+BONUS+OT_SUMMARY
	END)TOTAL_SALARY_INCOME,

	(CASE
		WHEN PAY_TYPE = 'MONTH' THEN SALARY*SOCIAL_INSURANCE/100
		ELSE DAILY*SOCIAL_INSURANCE/100
	END)SUBTRACT_SOCIAL,

	(CASE
		WHEN PAY_TYPE = 'MONTH' THEN SALARY*TAX/100
		ELSE DAILY*TAX/100
	END)SUBTRACT_TAX,

	(CASE
		WHEN PAY_TYPE = 'MONTH' THEN (SALARY*SOCIAL_INSURANCE/100)+(SALARY*TAX/100)
		ELSE (DAILY*SOCIAL_INSURANCE/100)+(DAILY*TAX/100)
	END)TOTAL_SUBTRACT,

	(CASE
		WHEN PAY_TYPE = 'MONTH' THEN (SALARY+FARE+DILIGENCE+BONUS+OT_SUMMARY)-(SALARY*SOCIAL_INSURANCE/100)-(SALARY*TAX/100)
		ELSE (DAILY+FARE+DILIGENCE+BONUS+OT_SUMMARY)-(DAILY*SOCIAL_INSURANCE/100)-(DAILY*TAX/100)
	END)TOTAL_SALARY_INCOME_NET



FROM (
	SELECT 	U.USER_ID AS USER_ID,
		U.FIRST_NAME,
		U.LAST_NAME,
		U.DEPARTMENT AS POSITION,
		U.ID_CARD,
		U.PAY_TYPE,
		U.SALARY,
		U.SOCIAL_INSURANCE,
		U.TAX,
		IFNULL((SELECT COUNT(S.DAILY)	 						FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'daily'),0) AS DAILY_DAY,
		IFNULL((SELECT SUM(S.DAILY)	 						FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'daily'),0) AS DAILY,
		IFNULL((SELECT DISTINCT S.FARE 						FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'fare'),0) AS FARE,
		IFNULL((SELECT DISTINCT S.DILIGENCE 				FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = 10 AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'diligence'),0) AS DILIGENCE,
		IFNULL((SELECT DISTINCT S.BONUS 					FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'bonus'),0) AS BONUS,
		IFNULL((SELECT DISTINCT(U.SALARY/30/8) 				FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'ot'),0) AS OT_PER_HOUR,
		IFNULL((SELECT DISTINCT COUNT(S.OT_HOUR)			FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'ot'),0) AS OT_DATE,
		IFNULL((SELECT DISTINCT SUM(S.OT_HOUR)				FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'ot'),0) AS OT_HOUR,
		IFNULL((SELECT DISTINCT S.OT_RATE 					FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = 10 AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'ot'),0) AS OT_RATE,
		IFNULL((SELECT DISTINCT ((U.SALARY/30/8)*SUM(S.OT_HOUR)*OT_RATE) 	FROM SALARY_TRANSACTION S WHERE U.ID = S.USER_ID AND S.MONTH = $P{month} AND S.YEAR = $P{year} AND S.TRANSACTION_TYPE = 'ot'),0) AS OT_SUMMARY


	FROM 	USERS_PROFILE U
	WHERE 	1=1

)AS SUB_QUERY
WHERE 1=1
$P!{sql}]]>
